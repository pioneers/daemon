{
  "name": "Dawn",
  "version": "0.4.0-beta.3",
  "description": "Frontend for PIE Robotics System",
  "main": "main.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "watch": "node ./node_modules/webpack/bin/webpack.js --watch"
  },
  "betterScripts": {
    "start": {
      "command": "electron main.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "node ./node_modules/webpack/bin/webpack.js -p --config webpack.production.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "better-npm-run": "0.0.7",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "brace": "^0.7.0",
    "electron-json-storage": "^1.0.0",
    "flux": "^2.1.1",
    "json-loader": "^0.5.4",
    "keymirror": "^0.1.1",
    "lodash": "^4.3.0",
    "mousetrap": "^1.5.3",
    "numeral": "^1.5.3",
    "object-assign": "^4.0.1",
    "react": "^0.14.5",
    "react-ace": "^3.0.0",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.5",
    "react-edit-inline": "^1.0.6",
    "react-joyride": "^1.1.1",
    "smalltalk": "^1.6.5",
    "superagent": "^1.7.2"
  }
}
